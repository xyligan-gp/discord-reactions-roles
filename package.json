{
  "name": "discord-reactions-roles",
  "version": "1.0.5",
  "description": "A simple and powerful module for adding a reaction role system to your discord.js project",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.json",
    "build:old": "tsc -p tsconfig.json",
    "build:docs": "docgen -s src/*.ts src/**/*.ts -o docs/index.json -c docs/index.yml -g -S 1 -j .jsconfig.json",
    "pub": "npm publish",
    "pub:dev": "npm publish --tag dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xyligan-gp/discord-reactions-roles.git"
  },
  "author": "xyligan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xyligan-gp/discord-reactions-roles/issues"
  },
  "homepage": "https://drr.js.org",
  "dependencies": {
    "discord-emojis-parser": "^1.0.4",
    "discord-message-parser": "^1.0.1",
    "discord.js": "^14.11.0",
    "enmap": "^5.9.8"
  },
  "keywords": [
    "discord",
    "discord-bot",
    "discord.js",
    "reactions",
    "roles",
    "reactions-roles",
    "discord-reactions-roles",
    "xyligan"
  ],
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@distube/docgen": "github:distubejs/docgen",
    "jsdoc-babel": "^0.5.0",
    "rimraf": "^3.0.2",
    "typescript": "^5.1.3"
  },
  "engines": {
    "node": ">=16.9.0"
  },
  "types": "./types/ReactionsRoles.d.ts"
}
